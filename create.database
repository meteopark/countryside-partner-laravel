-- 마을 홍보 동영상
CREATE TABLE village_video(
BBSCTT_NO int not null,
VILAGE_ID varchar(255) not null,
SJ varchar(255) not null,
WRTER varchar(10) not null,
CN longtext not null,
CTPRVN varchar(255) not null,
GU_GUN varchar(255) not null,
THUMB_URL_COURS1 varchar(255) not null,
THUMB_URL_COURS2 varchar(255) not null,
THUMB_URL_COURS3 varchar(255) not null,
THUMB_URL_COURS4 varchar(255) not null,
PRIMARY KEY(VILAGE_ID, BBSCTT_NO)
)ENGINE = MyISAM, CHARACTER SET = UTF8;

